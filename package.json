{
    "name": "github-line-share",
    "displayName": "Github Line Share",
    "description": "share a link to github source automatically",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.shareGithubLine"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.shareGithubLine",
                "title": "Share Github Line"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.1.4",
        "vscode": "^1.1.25",
        "eslint": "^4.11.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42"
    }
}
